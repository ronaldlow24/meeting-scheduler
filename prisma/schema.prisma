generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MeetingRoom {
  id        String   @id @default(cuid())
  title     String   @db.Text
  startTime DateTime
  endTime   DateTime
  actualStartTime DateTime
  actualEndTime DateTime
  createdAt DateTime
}


